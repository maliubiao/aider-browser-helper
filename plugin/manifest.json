{
    "manifest_version": 3,
    "name": "URL HTML Fetcher",
    "version": "1.0",
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "tabs",
        "activeTab",
        "scripting"
    ],
    "host_permissions": [
        "http://localhost:8888/*"
    ]
}
